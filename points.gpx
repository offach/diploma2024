<?xml version="1.0" encoding="UTF-8"?>
<gpx version="1.1" creator="Your Name" xmlns="http://www.topografix.com/GPX/1/1">
  <metadata>
    <name>My GPX File</name>
  </metadata>
  <!-- Генерация случайных точек -->
  {% for i in range(100) %}
    <wpt lat="{{ getRandom(-90, 90) }}" lon="{{ getRandom(-180, 180) }}">
      <name>Point {{ i + 1 }}</name>
      <desc>Random Point {{ i + 1 }}</desc>
    </wpt>
  {% endfor %}
</gpx>
